<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.akash">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_akash"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_akash"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayAccessory.DisplayAccessory"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisplayProductGroup.DisplayProductGroup"
            android:label="@string/title_activity_display_images"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisplayProduct.DisplayProduct"
            android:label="@string/title_activity_show_image"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FAQ.FAQ"
            android:label="@string/title_activity_faq"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Delete"
            android:label="@string/title_activity_delete"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".services.Services"
            android:label="@string/title_activity_services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".aboutus.AboutUs"
            android:label="@string/title_activity_aboutus"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>